    <script type="text/javascript">
        function showModal(value) {
            var modals = document.querySelectorAll('.modal');

            // Tüm modalları gizlemek için klasik bir for döngüsü kullan
            for (var i = 0; i < modals.length; i++) {
                modals[i].style.display = 'none';
            }

            // Seçilen modalı göster
            var modalName = document.getElementById(value);
            if (modalName) {
                modalName.style.display = 'flex';
            }
        }

        // Tüm modalları kapatma fonksiyonu
        function closeAllModals() {
            var modals = document.querySelectorAll('.modal');

            // Klasik for döngüsü ile modalları gizle
            for (var i = 0; i < modals.length; i++) {
                modals[i].style.display = 'none';
            }
        }

        // close-modal sınıfına sahip butonlara tıklanınca tüm modalları kapat
        var closeButtons = document.querySelectorAll('.close-modal');
        for (var i = 0; i < closeButtons.length; i++) {
            closeButtons[i].addEventListener('click', closeAllModals);
        }

        var closeAggrement = document.querySelector('.reject-agreement');
        closeAggrement.addEventListener('click', function () {
            closeAllModals();
            window.location.href = '/logingame.aspx';

        })

        // Modalın dışına tıklanırsa modalları kapat
        var modals = document.querySelectorAll('.modal');
        for (var i = 0; i < modals.length; i++) {
            modals[i].addEventListener('click', function (e) {
                if (e.target === this) {
                    closeAllModals();
                }
            });
        }

        // Escape tuşuna basıldığında modalları kapat
        document.addEventListener('keydown', function (e) {
            if (e.key === 'Escape') {
                closeAllModals();
            }
        });
    </script>
</body>
