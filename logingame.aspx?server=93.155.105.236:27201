        function OnAdRegister() {
            setTimeout('document.location = "logingame.aspx";', 3000);
        }

        function TxtFocus(obj, text) {
            if (obj.value == text) {
                obj.value = "";
            }
        }

        function TxtBlur(obj, text) {
            if (obj.value == "") {
                obj.value = text;
            }
        }

        function BadLogin() {
            document.getElementById("wrongUserName").style.display = "none";
            document.getElementById("badLogin").style.display = "block";
            document.getElementById("notActivated").style.display = "none";
            document.getElementById("notActivatedPhone").style.display = "none";
            document.getElementById("wrongsec").style.display = "none";
            document.getElementById("txtPasword").text = "";
            document.getElementById("txtSecImage").text = "";
        }

        function WrongUserName() {
            document.getElementById("wrongUserName").style.display = "block";
            document.getElementById("badLogin").style.display = "none";
            document.getElementById("notActivated").style.display = "none";
            document.getElementById("notActivatedPhone").style.display = "none";
            document.getElementById("wrongsec").style.display = "none";
            document.getElementById("txtPasword").text = "";
            document.getElementById("txtSecImage").text = "";
        }

        function NotActivated() {
            document.getElementById("wrongUserName").style.display = "none";
            document.getElementById("badLogin").style.display = "none";
            document.getElementById("notActivated").style.display = "block";
            document.getElementById("notActivatedPhone").style.display = "none";
            document.getElementById("wrongsec").style.display = "none";
        }
        function NotActivatedPhone() {
            document.getElementById("wrongUserName").style.display = "none";
            document.getElementById("badLogin").style.display = "none";
            document.getElementById("notActivated").style.display = "none";
            document.getElementById("notActivatedPhone").style.display = "block";
            document.getElementById("wrongsec").style.display = "none";
        }
        function WrongSecurity() {
            document.getElementById("wrongUserName").style.display = "none";
            document.getElementById("badLogin").style.display = "none";
            document.getElementById("notActivated").style.display = "none";
            document.getElementById("notActivatedPhone").style.display = "none";
            document.getElementById("wrongsec").style.display = "block";
            document.getElementById("txtSecImage").text = "";
            document.getElementById("txtSecImage").focus();
        }


